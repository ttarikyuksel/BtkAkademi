@model List<StoreApp.Models.Product>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Documents</title>
</head>
<body>
    @foreach (var item in Model)
    {
        <div>
            <h3>@item.ProductName</h3>
            <p>@item.Price</p>
            <p>
                <a href="/Product/Get/@item.Id">Get Details</a>
            </p>
        </div>
    }
</body>
</html>



